    "react": "^19.1.0",
    "react-compiler-runtime": "^19.1.0-rc.2",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
