  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
